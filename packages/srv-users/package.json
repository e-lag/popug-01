{
  "name": "popug.users",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "main": "dist/index.js",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build -p tsconfig.json",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "format": "eslint \"src/**/*.ts\" --fix",
    "lint": "eslint \"src/**/*.ts\"",
    "migration:create": "npx mikro-orm migration:create",
    "migration:down": "npx mikro-orm migration:down",
    "migration:up": "npx mikro-orm migration:up",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "engines": {
    "node": ">=18.12.1",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^4.0.0",
    "@mikro-orm/core": "^5.7.12",
    "@mikro-orm/nestjs": "^5.2.0",
    "@mikro-orm/migrations": "^5.7.12",
    "@mikro-orm/postgresql": "^5.7.12",
    "@mikro-orm/cli": "^5.7.14",
    "@nestjs/common": "^10.1.2",
    "@nestjs/swagger": "^7.1.4",
    "@nestjs/config": "^3.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/cqrs": "^10.0.3",
    "@nestjs/core": "^10.1.2",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.2",
    "@sendgrid/mail": "^7.7.0",
    "jsonwebtoken": "^9.0.1",
    "handlebars": "^4.7.7",
    "nestjs-zod": "^2.3.3",
    "nest-winston": "^1.9.3",
    "cls-hooked": "^4.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "dotenv": "16.0.1",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "ms": "^2.1.3",
    "passport": "^0.6.0",
    "pbkdf2": "^3.1.2",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.21.4",
    "zod-http-schemas": "^2.0.0",
    "zod-to-json-schema": "^3.21.2",
    "zod-endpoints": "^0.1.7",
    "@popug/utils-common": "0.1.0",
    "@popug/utils-micro-orm": "0.1.0"
  },
  "devDependencies": {
    "handlebars": "^4.7.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts"
    ]
  }
}
